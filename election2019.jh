entity Personne {
	Cni String
}

entity Candidat {
	nom String required,
    prenom String required,
}



relationship OneToOne {
	Personne{region} to Candidat
}
// Set pagination options
paginate Personne, Candidat with infinite-scroll
paginate Job with pagination

dto * with mapstruct

// Set service options to all except few
service all with serviceImpl except Personne,Candidat
// Set an angular suffix
angularSuffix * with mySuffix
